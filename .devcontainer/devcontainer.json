{
    "name": "ROS Monocular Odometry",
    "dockerComposeFile": "docker-compose.yml",
    "service": "ros2",
    "remoteUser": "ros",
    "workspaceFolder": "/home/ros/workspace",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.cpptools",
                "ms-vscode.cpptools-themes",
                "twxs.cmake",
                "donjayamanne.python-extension-pack",
                "eamodio.gitlens",
                "ms-iot.vscode-ros"
            ]
        }
    },
    "containerEnv": {
        "ROS_AUTOMATIC_DISCOVERY_RANGE": "LOCALHOST",
        "ROS_DOMAIN_ID": "28",
        "_colcon_cd_root": "/home/ros/workspace"
    },
    "postCreateCommand": "sudo rosdep update && sudo rosdep install --from-paths src --ignore-src -y"
}